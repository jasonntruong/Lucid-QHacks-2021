<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lucidmain">

    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-feature android:name="android.hardware.Camera" android:required="true"></uses-feature>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/lucidlogo"
        android:label="Lucid"
        android:roundIcon="@mipmap/lucidlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.LucidMain">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>